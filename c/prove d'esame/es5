typedef struct nodo{
    char carattere;
    int contatore;
    struct nodo *next;
} nodo;

int main(){
    char a = "cuay";
    riordinastringa(a);
}

char riordinastringa(char s[]){
    int lunghezza = strlen(*s);
    char *risultato = malloc(sizeof(char)*lunghezza);
    printf("%d",lunghezza);

}

